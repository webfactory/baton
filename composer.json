{
    "name": "webfactory/baton",
    "type": "project",
    "description": "Composer Dependency Tracking",
    "require": {
        "symfony/symfony": "^3.4.47",
        "symfony/monolog-bundle": "^3.2.0",
        "sensio/framework-extra-bundle": "~3.0",
        "doctrine/orm": "^2.5.14",
        "doctrine/doctrine-bundle": "^1.8.1",
        "composer/composer": "^1.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "doctrine/doctrine-fixtures-bundle": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "7.2.24"
        }
    }
}
