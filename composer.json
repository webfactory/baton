    "require": {
        "symfony/symfony": "~3.4",
        "symfony/monolog-bundle": "^3.2.0",
        "sensio/framework-extra-bundle": "~3.0",
        "doctrine/orm": "^2.5.14",
        "doctrine/doctrine-bundle": "^1.8.1",
        "composer/composer": "^1.6"
    },

    "require-dev": {
