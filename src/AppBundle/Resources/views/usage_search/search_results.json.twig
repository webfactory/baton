{
    "operator": "{{ operator }}",
    "versionString": "{{ versionString }}",
    "package": {
        "id": "{{ package.id }}",
        "name": "{{ package.name }}",
        "versions": [
            {% for packageVersion in matchingPackageVersions %}
            {
                "prettyVersion": "{{ packageVersion.prettyVersion }}",
                "projects": [
                    {% for project in packageVersion.projects %}
                    {
                        "id": "{{ project.id }}",
                        "name": "{{ project.name }}"
                    }{% if not loop.last %},{% endif %}
                    {% endfor %}
                ]
            }{% if not loop.last %},{% endif %}
            {% endfor %}
        ]
    }
}
